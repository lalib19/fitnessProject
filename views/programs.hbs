<h1 class="title">Programs</h1>
<hr>

<ul class="exoList">
    {{#each programs}}
    <li class="exercise">
        <p>{{this.name}}</p>
        <p>{{this.description}}</p>
        <p>{{this.exercisesList}}</p>
        <p>{{this.creator}}</p>
        <p>{{this.likes}}</p>
    </li>
    {{/each}}
</ul>