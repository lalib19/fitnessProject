<h1 class="title">Exercises</h1>
<hr>


<ul class="programList">
    {{#each programs}}
    <li>
        <ul class="exoList">
            {{#each favorites}}
            <li class="favorites">
                <p>{{this.name}}</p>
                <p>{{this.description}}</p>
                <p>{{this.level}}</p>
                <p>{{this.bodyPart}}</p>
                <iframe width="420" height="315" src={{this.exerciseUrl}}>
                </iframe>
                {{!-- <a class="link" href="/exercises/page/{{this._id}}">... see more</a> --}}
                <a href="/foods/edit/{{this._id}}">Edit</a>
                <a href="/delete/{{this._id}}">Delete</a>
            </li>
            {{/each}}
        </ul>
    </li>
    {{/each}}
</ul>