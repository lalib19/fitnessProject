<h1 class="title">Exercises of {{programName}}</h1>
<hr>

<ul class="exoList">
    {{#each exercises}}
    <li class="exercise">
        <p>{{this.name}}</p>
        <p>{{this.description}}</p>
        <p>{{this.level}}</p>
        <p>{{this.bodyPart}}</p>
        <iframe width="420" height="315" src={{this.exerciseUrl}}>
        </iframe>
        {{!-- <a class="link" href="/exercises/page/{{this._id}}">... see more</a> --}}
    </li>
    {{/each}}
</ul>