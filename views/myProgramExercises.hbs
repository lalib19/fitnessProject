

<div class="myProgramsExercises-container">

    {{!-- <h1 id="myUnique-exercise-h1" class="title">WELCOME BACK! HERE IS {{programName}} PROGRAM</h1> --}}

    <ul class="exoList exoList-exercises">
        {{#each exercises}}
        <li class="exercise myUnique-exercise">
            <div class="myUnique-exercise-upper-info">
                <p>{{this.name}}</p>
                <p>{{this.level}}</p>
                <p>{{this.bodyPart}}</p>
            </div>
            <iframe width="100%" height="315" src={{this.exerciseUrl}}>
            </iframe>
            <div class="myUnique-exercise-bottom-info">
                <p>{{this.description}}</p>
            </div>
            {{!-- <a class="link" href="/exercises/page/{{this._id}}">... see more</a> --}}
        </li>
        {{/each}}
    </ul>   
</div>

